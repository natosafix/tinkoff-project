<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="create-rn-processed-images" author="natosafix">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="rn_processed_images"/>
            </not>
        </preConditions>

        <createTable tableName="rn_processed_images">
            <column name="request_id" type="uuid">
                <constraints nullable="false"/>
            </column>
            <column name="image_id" type="uuid">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addPrimaryKey tableName="rn_processed_images" columnNames="request_id,image_id"
                       constraintName="rn_processed_images_request_image_primary_key_constraint"/>
    </changeSet>
</databaseChangeLog>